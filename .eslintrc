{
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 9
  },
  "env": {
    "jest": true,
    "node": true
  },
  "plugins": ["prettier"],
  "rules": {
    "camelcase": 0,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "import/prefer-default-export": 0,
    "no-console": 0,
    "no-restricted-syntax": 0,
    "no-unused-vars": [2, {"args": "after-used", "ignoreRestSiblings": true, "varsIgnorePattern": '^_'}]
  }
}
